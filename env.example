# Umami Analytics Configuration
# Copy this file to .env.local for local development
# and .env.production for production deployment

# Umami Analytics Domain
# Local development: http://localhost:3000
# Production: https://monitor.kommeterr.dev
VITE_UMAMI_DOMAIN=http://localhost:3000

# Umami Website ID (get this from Umami dashboard)
# Leave empty for first setup, then add your actual website ID
VITE_UMAMI_WEBSITE_ID=

# Umami Admin Configuration (for docker-compose.yml)
UMAMI_ADMIN_EMAIL=admin@kommeterr.dev
UMAMI_ADMIN_PASSWORD=changeme123

# Database Configuration
POSTGRES_DB=umami
POSTGRES_USER=umami
POSTGRES_PASSWORD=umami